{
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@7.3.0",
  "description": "test for ...",
  "main": "index.js",
  "license": "MIT",
  "keywords": [],
  "author": "",
  "workspaces": [
    "packages/*",
    "example",
    "docs"
  ],
  "scripts": {
    "cz": "czg",
    "test": "pnpm --filter components test",
    "test:coverage": "pnpm --filter components coverage",
    "test:ssr": "vitest --config ./ssr-testing/vitest.config.ts",
    "prepare:e2e": "if [ ! -d \"docs/.vitepress/dist\" ]; then pnpm run docs:build; fi;",
    "commit": "git-cz",
    "dev": "pnpm -C play dev --port 3000",
    "build:justforfun": "gulp -f packages/components/build/gulpfile.ts",
    "build": "pnpm --filter @justforfun-ui/components build:vite",
    "format": "prettier --write --cache .",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json && prettier-quick --branch dev",
    "runall": "pnpm run typecheck:web && pnpm run typecheck:play",
    "typecheck": "run-p typecheck:web typecheck:play typecheck:node",
    "typecheck:node": "tsc -p tsconfig.node.json --noEmit",
    "typecheck:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit",
    "typecheck:play": "vue-tsc -p tsconfig.play.json --composite false --noEmit",
    "lint:commit": "commitlint --from $(git merge-base origin/dev HEAD) --to HEAD > ./commit-lint.txt",
    "deploy": "bash deploy.sh",
    "docs:dev": "vitepress dev docs --port 9000",
    "docs:build": "vitepress build docs",
    "docs:serve": "pnpm --filter @justforfun-ui/docs docs:serve",
    "docs:preview": "vitepress preview docs --port 8989",
    "preserve": "husky install"
  },
  "peerDenpendencies": {
    "vue": ">= 3.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@justforfun-ui/components": "workspace:^1.0.2",
    "@justforfun-ui/constants": "workspace:*",
    "@justforfun-ui/eslint-config": "workspace:^0.0.1",
    "@justforfun-ui/theme-chalk": "workspace:^1.0.0",
    "@justforfun-ui/tokens": "workspace:^0.0.1",
    "@justforfun-ui/utils": "workspace:^1.0.0",
    "@vueuse/core": "^9.13.0",
    "dayjs": "^1.11.7",
    "json": "^11.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@justforfun-ui/hooks": "workspace:^1.0.0",
    "@pnpm/find-workspace-packages": "^5.0.34",
    "@pnpm/logger": "^5.0.0",
    "@pnpm/types": "^8.10.0",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-less": "^0.0.32",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.0",
    "@types/sass": "^1.43.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/ui": "0.28.5",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.13.0",
    "chalk": "^5.2.0",
    "child_process": "^1.0.2",
    "concurrently": "^7.6.0",
    "consola": "^2.15.3",
    "csstype": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.4.1",
    "czg": "^1.4.1",
    "eslint": "^8.34.0",
    "eslint-define-config": "^1.15.0",
    "expect-type": "^0.15.0",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.3",
    "jsdom": "21.1.0",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "puppeteer": "^19.7.1",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^4.1.2",
    "sass": "^1.58.3",
    "sucrase": "^3.29.0",
    "ts-morph": "^17.0.1",
    "tsx": "^3.12.3",
    "type-fest": "^3.6.0",
    "typescript": "^4.9.5",
    "unplugin-vue-define-options": "^1.2.3",
    "unplugin-vue-macros": "^1.9.0",
    "vitepress": "1.0.0-alpha.47",
    "vitest": "0.28.5",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.1.3"
  },
  "engines": {
    "node": ">= 16"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "react",
        "react-dom"
      ]
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
